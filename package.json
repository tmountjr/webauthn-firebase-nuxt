{
  "scripts": {
    "edgio:dev": "node setNodeModules.js && edgio dev",
    "edgio:build": "node setNodeModules.js && edgio build",
    "edgio:deploy": "node setNodeModules.js && edgio deploy",
    "edgio:prod": "npm run edgio:build && edgio run -p",
    "dev": "nuxt",
    "https-dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && nuxt --env.NODE_TLS_REJECT_UNAUTHORIZED=0",
    "build": "nuxt build",
    "start": "nuxt start",
    "https-start": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && nuxt start --env.NODE_TLS_REJECT_UNAUTHORIZED=0",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js --fix"
  },
  "name": "webauthn-firebase-nuxt",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@nuxt/core": "^2.15.8",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/firebase": "^8.2.2",
    "@simplewebauthn/server": "^6.2",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4",
    "firebase": "^9.9.4",
    "parse-url": "^8",
    "universal-cookie": "^4.0.4",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@edgio/cli": "^5.0.0",
    "@edgio/core": "^5.0.0",
    "@edgio/devtools": "^5.0.0",
    "@edgio/nuxt": "^5.0.0",
    "@edgio/prefetch": "^5.0.0",
    "@edgio/vue": "^5.0.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@simplewebauthn/browser": "^6.2",
    "@vercel/nft": "^0.22.1",
    "base64url": "^3.0.1",
    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.21.2",
    "chart.js": "^3",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.19.3",
    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "nuxt": "^2.15.8",
    "sass": "^1.54.9",
    "sass-loader": "^10",
    "vue": "^2.6.14",
